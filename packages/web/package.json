{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "main": "n/a",
  "scripts": {
    "gen-schema-types": "yarn apollo-codegen generate **/*.tsx --schema schema.json --target typescript --output src/__generated__/",
    "gen-schema": "yarn apollo-codegen introspect-schema http://localhost:4000/graphql --output schema.json",
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "yarn gen-schema && yarn gen-schema-types && gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "gatsby develop",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.3.7",
    "apollo-client": "^2.4.4",
    "apollo-link-http": "^1.5.5",
    "apollo-link-persisted-queries": "^0.2.1",
    "babel-plugin-styled-components": "^1.7.1",
    "classnames": "^2.2.6",
    "emotion": "^9.2.9",
    "emotion-server": "^9.2.9",
    "gatsby": "^2.0.3",
    "gatsby-image": "^2.0.5",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-emotion": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-typescript": "^2.0.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.1",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "graphql-tag": "^2.10.0",
    "isomorphic-fetch": "^2.2.1",
    "modern-normalize": "^0.5.0",
    "normalize.css": "^8.0.0",
    "polished": "^2.0.3",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^16.6.0",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.9",
    "react-helmet": "^5.2.0",
    "typescript": "^3.0.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.7",
    "gh-pages": "^2.0.0",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
  }
}
